{
    "name": "dev-tool/laravel-dev-tool",
    "description": "model-annotation、ide-helper...",
    "keywords": ["model ide"],
    "type": "library",
    "license": "MIT",
    "authors": [{
        "name": "chenzuming",
        "email": "403502556@qq.com"
    }],
    "require": {
        "php": ">=7.3.0",
        "barryvdh/reflection-docblock": "~2.0.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.0"
    },
    "autoload": {
        "psr-4": {
            "DevTool\\LaravelDevTool\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "DevTool\\LaravelDevTool\\DevToolServiceProvider"
            ]
        }
    }
}